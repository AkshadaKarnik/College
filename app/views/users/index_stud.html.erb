<h1>Student Profile</h1>
<table class="table">
  <thead class="thead-light">
    <tr>
      <th scope="col">Email</th>
      <th scope="col">Role</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @students.each do |stud| %>
      <tr>
        <td><%= stud.name %></td>
        <td><%= stud.email %></td>
        <td><%= stud.role %></td>
      </tr>
      <% end %>
    </tr>
  </tbody>
</table>