<%= form_with model: @college do |form| %>
  <% if @college.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(@college.errors.count, "error") %> prohibited this college from being saved:</h2>

      <ul>
        <% @college.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group">
    <%= form.text_field :name, class:"form-control", placeholder:"College Name"  %>
  </div>
  <div class="form-group">
    <%= form.text_field :email, class:"form-control", placeholder:"Email" %>
  </div>
  <div class="form-group">
    <%= form.text_field :address, class:"form-control", placeholder:"College Address" %>
  </div>
  <div class="form-group">
    <%= form.text_field :contact, class:"form-control", placeholder:"Contact"  %>
  </div>
  <div class="form-group">
    <%= form.submit :Submit, class:'btn btn-success' %>
  </div>
<% end %>