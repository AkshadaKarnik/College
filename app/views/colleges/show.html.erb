<div class="jumbotron">
  <h1 class="display-4">
    <%= @college.name %>
  </h1>
  <p class="lead">
    <%= @college.email %> |
    <%= @college.address%> |
    <%= @college.contact%>
  </p>
</div>
<%= link_to 'edit', edit_college_path(@college), class:"btn btn-secondary" %>
<%= link_to 'back to colleges',  colleges_path, class:"btn btn-secondary" %>
<%= link_to 'delete', @college, data: {turbo_method: :delete,turbo_confirm: "Are you sure?" }, class:"btn btn-danger" %>